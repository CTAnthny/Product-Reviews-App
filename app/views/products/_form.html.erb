<%= bootstrap_form_for @product do |f| %>
  <%= render 'shared/error_messages', object: @product %>

  <%= f.text_field :name, class: 'form-control' %>

  <%= f.text_area :description, class: 'form-control' %>

  <%= f.text_field :website, class: 'form-control' %>

  <%= f.primary "Submit" %>

<% end %>
